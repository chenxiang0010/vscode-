{
  "el-表单二次验证": {
    "prefix": ["el-form-validate"],
    "body": [
      "this.$$refs.${1:form标签中设置的ref=\"自定义\"}.validate(async valid => {",
      "\tif (!valid) return",
      "\t//如果验证通过执行下面代码",
      "\t${0:二次验证通过执行代码(ajax)}",
      "})"
    ],
    "description": ["el-表单二次验证"]
  },
  "el-cascader-props配置": {
    "prefix": ["el-cascader-props"],
    "body": [
      "cascaderProps: {",
      "    label: '${1:显示的名字}',",
      "    value: '${2:提交时对应的唯一Id值}',",
      "    children: '${3:对应子选项}'",
      "    expandTrigger: 'hover' //hover 触发子菜单",
      "}"
    ],
    "description": ["el-cascader-props配置"]
  },
  "消息提示框": {
      "prefix": ["el-message"],
      "body": [
        "this.$message({",
        "    message: message,",
        "    type: 'success'",
        "})"
      ],
      "description": [
      "el-message"
      ]
  },
  "确认提示框": {
      "prefix": ["el-confirm"],
      "body": [
          "this.$confirm('此操作将永久删除该文件, 是否继续?', '提示', {",
          "  confirmButtonText: '确定',",
          "  cancelButtonText: '取消',",
          "  type: 'warning'",
          "}).then(() => {",
          "  this.$message({",
          "    type: 'success',",
          "    message: '删除成功!'",
          "  })",
          "}).catch(() => {",
          "  this.$message({",
          "    type: 'info',",
          "    message: '已取消删除'",
          "  })",
          "})"
      ],
      "description": [
      "确认提示框"
      ],
    "通知框": {
        "prefix": ["el-notify"],
        "body": [
            "this.$notify({",
            "    title: '成功',",
            "    message: '这是一条成功的提示消息',",
            "    type: 'success'",
            "})"
        ],
        "description": [
        "通知框"
        ]
    }
  }
}
