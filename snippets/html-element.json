  "el-button": {
    "prefix": ["el-button"],
    "body": [
      "<el-button type=\"${1|primary,success,info,warning,danger|}\" ${2|浅色,plain|} ${3|样式(圆角/圆),round,circle|} ${4:disabled}>$0</el-button>"
    ],
    "description": ["el-button"]
  },
  "el-link": {
    "prefix": ["el-link"],
    "body": [
      "<el-link href=\"#\" type=\"${1|primary,success,info,warning,danger|}\" ${2:disabled} ${3|无下划线,:underline=\"false\"|} ${4|带文字前图标,icon=\"el-icon-edit\"|}>$0</el-link>"
    ],
    "description": ["el-link"]
  },
  "el-radio": {
    "prefix": ["el-radio"],
    "body": [
      "<el-radio v-model=\"$1\" label=\"${2:str/num/bool}\" ${3|带边框,border|} @change=\"${4}\">${0:备选项}</el-radio>"
    ],
    "description": ["单选框"]
  },
  "el-radio-group": {
    "prefix": ["el-radio-group"],
    "body": [
      "<el-radio-group v-model=\"radio\" ${1|选中时的文本颜色,text-color=\"\"|} ${2|选中时的填充色和边框,fill=\"\"|} ${3:@change=\"\"}>",
      "  <el-radio :label=\"1\">备选项</el-radio>",
      "  <el-radio :label=\"2\">备选项</el-radio>",
      "  <el-radio :label=\"3\">备选项</el-radio>",
      "</el-radio-group>"
    ],
    "description": ["单选组"]
  },
  "el-radio-button": {
    "prefix": ["el-radio-button"],
    "body": [
      "<el-radio-group v-model=\"radio\" size=\"${1|medium,mini,small|}\">",
      "    <el-radio-button label=\"上海\"></el-radio-button>",
      "    <el-radio-button label=\"北京\"></el-radio-button>",
      "    <el-radio-button label=\"广州\"></el-radio-button>",
      "    <el-radio-button label=\"深圳\"></el-radio-button>",
      "</el-radio-group>"
    ],
    "description": ["btn样式的单选框"]
  },
  "el-checkbox-group": {
    "prefix": ["el-checkbox-group"],
    "body": [
      "<el-checkbox-group v-model=\"checkList\" ${1|最大值最小值,:min=\"1\" :max=\"2\"|}>",
      "  <el-checkbox label=\"复选框 A\"></el-checkbox>",
      "  <el-checkbox label=\"复选框 B\"></el-checkbox>",
      "  <el-checkbox label=\"复选框 C\"></el-checkbox>",
      "  <el-checkbox label=\"禁用\" disabled></el-checkbox>",
      "  <el-checkbox label=\"选中且禁用\" disabled></el-checkbox>",
      "</el-checkbox-group>"
    ],
    "description": ["复选框组"]
  },
  "el-input": {
    "prefix": ["el-input"],
    "body": [
      "<el-input v-model=\"input\" placeholder=\"请输入内容\" ${1|可清空,clearable|} ${2|密码框,show-password|} ${3|首尾显示图标,prefix-icon=\"el-icon-search\",suffix-icon=\"el-icon-date\"|}></el-input>"
    ],
    "description": ["el-input"]
  },
  "el-textarea": {
    "prefix": ["el-textarea"],
    "body": [
      "<el-input",
      "  type=\"textarea\"",
      "  :rows=\"2\"",
      "  $1{|根据文字内容自适应,autosize,:autosize=\"{ minRows: 2, maxRows: 4}\"|}",
      "  placeholder=\"请输入内容\"",
      "  v-model=\"textarea\">",
      "</el-input>"
    ],
    "description": ["el-textarea"]
  },
  "el-input-number": {
    "prefix": ["el-input-number"],
    "body": [
      "<template>",
      "  <el-input-number v-model=\"num\"",
      "                   @change=\"handleChange\"",
      "                   size=\"${1|medium,small,mini|}\"",
      "                   :min=\"1\"",
      "                   :max=\"10\"",
      "                   ${2|步长(number),:step=\"2\"|}",
      "                   ${3|严格步数,step-strictly|}",
      "                   ${4|数值精度,不能小于步长小数位数(小数几位),:precision=\"2\"|}",
      "                   ${5|按钮位置,controls-position=\"right\"|}",
      "                   label=\"描述文字\"></el-input-number>",
      "</template>"
    ],
    "description": ["计数器"]
  },
  "el-select": {
    "prefix": ["el-select"],
    "body": [
      "<template>",
      "  <el-select v-model=\"${1:选中结果}\" placeholder=\"请选择\">",
      "    <el-option",
      "      v-for=\"item in ${2:option}\"",
      "      :label=\"item.label\"",
      "      :value=\"item.value\"",
      "      ${3|禁用项(option中添加disabled: true),:disabled=\"item.disabled\"|}",
      "      ${4|可清空单选,clearable|}",
      "      :key=\"item.value\">",
      "    </el-option>",
      "  </el-select>",
      "</template>"
    ],
    "description": ["Select 选择器"]
  },
  "el-select-multiple": {
    "prefix": ["el-select-mutiple"],
    "body": [
      "<el-select v-model=\"value1\"",
      "           multiple",
      "           ${1|输入文字来创建新的条目,allow-create filterable|}",
      "           ${2|回车选中当前选项,default-first-option|}",
      "           placeholder=\"请选择\">",
      "  <el-option v-for=\"item in options\"",
      "             :key=\"item.value\"",
      "             :label=\"item.label\"",
      "             :value=\"item.value\">",
      "  </el-option>",
      "</el-select>"
    ],
    "description": ["el-select-multiple多选"]
  },
  "el-select-group": {
    "prefix": ["el-select-group"],
    "body": [
      "<template>",
      "  <el-select v-model=\"value\" placeholder=\"请选择\">",
      "    <el-option-group",
      "      v-for=\"group in options\"",
      "      :key=\"group.label\"",
      "      :label=\"${1|分组名,group.label|}\">",
      "      <el-option",
      "        v-for=\"item in group.options\"",
      "        :key=\"item.value\"",
      "        :label=\"${2|分组名,item.label|}\"",
      "        :value=\"item.value\">",
      "      </el-option>",
      "    </el-option-group>",
      "  </el-select>",
      "</template>"
    ],
    "description": ["el-select-group分组"]
  },
  "el-cascader": {
    "prefix": ["el-cascader"],
    "body": [
      "<el-cascader",
      "  v-model=\"${1|绑定一个空数组,value|}\"",
      "  :options=\"${2:渲染成级联的数据对象}\"",
      "  ${3|可清空,clearable|}",
      "  ${4|仅显示最后一级,:show-all-levels=\"false\"|}",
      "  ${5|配置级联选择框,:props=\"cascaderProps\"|}",
      "  ${6|hover选中,expand-trigger=\"hover\"|}",
      "  @change=\"handleChange\"></el-cascader>"
    ],
    "description": ["cascader级联选择器"]
  },
  "el-switch": {
    "prefix": ["el-switch"],
    "body": [
      "<el-switch",
      "  v-model=\"${1:boolen}\"",
      "  ${2|禁用,disabled|}",
      "  active-color=\"#13ce66\"",
      "  inactive-color=\"#ff4949\">",
      "</el-switch>"
    ],
    "description": ["el-switch开关"]
  },
  "el-slider": {
    "prefix": ["el-slider"],
    "body": [
      "<el-slider v-model=\"value3\" ${1|禁用tooltip提示,:show-tooltip=\"false\"|} ${2|设置步长,:step=\"10\"|} ${3|显示步长断点,show-stops|} ${4|开启范围选择(v-model绑定的为数组),range :max=\"10\"|} ${5|竖向,vertical height=\"200px\"|} ${6|展示标记,:marks=\"marks\"|}></el-slider>"
    ],
    "description": ["el-slider滑块"]
  },
  "el-tabs": {
    "prefix": ["el-tabs"],
    "body": [
      "<el-tabs v-model=\"${1|绑定激活项（字符串值为tab-pane中的name）,activeName|}\"",
      "         @tab-click=\"${2|传入activeName值,tabHandleClick|}\">",
      "  <el-tab-pane label=\"tab1名字\"",
      "               name=\"first\">动态参数</el-tab-pane>",
      "  <el-tab-pane label=\"tab2名字\"",
      "               name=\"second\">静态属性</el-tab-pane>",
      "</el-tabs>"
    ],
    "description": ["el-tabs"]
  },
  "el-dialog": {
    "prefix": ["el-dialog"],
    "body": [
      "<el-dialog title=\"添加\"",
      "           :visible.sync=\"dialogVisible\"",
      "           width=\"50%\">",
      "   <span>这是一个dialog</span>",
      "  <span slot=\"footer\"",
      "        class=\"dialog-footer\">",
      "    <el-button @click=\"dialogVisible = false\">取 消</el-button>",
      "    <el-button type=\"primary\"",
      "               @click=\"dialogVisible = false\">确 定</el-button>",
      "  </span>",
      "</el-dialog>"
    ],
    "description": ["el-dialog"]
  },
  "el-table": {
    "prefix": ["el-table"],
    "body": [
      "<el-table :data=\"${1|绑定的数据,tableData|}\"",
      "          style=\"width: 100%\">",
      "  <el-table-column type=\"${2|设置类型,expand,index,selection|}\"> ",
      "  </el-table-column>",
      "  <el-table-column prop=\"${3:渲染绑定的表数据tableDate中的哪个数据}\"",
      "                   label=\"${4:显示名字}\"",
      "                   width=\"180\">",
      "  </el-table-column>",
      "</el-table>"
    ],
    "description": ["el-table"]
  },
  "el-scrollbar": {
      "prefix": ["el-scrollbar"],
      "body": [
        "<el-scrollbar wrap-class=\"scrollbar-wrapper\" style=\"height:100%\">",
        "<!-- 滚动条包裹部分 -->",
        "\n$0",
        "</el-scrollbar>"
      ],
      "description": [
      "el-scrollbar滚动条"
      ]
  }
}
