{
  "el-tree": {
    "prefix": ["el-tree"],
    "body": [
      "<!-- 树形控件 -->",
      "<el-tree :data=\"${1:树形控件展示数据}\"",
      "        :props=\"${2:配置树中每层对应树中哪个数据(单独建array)}\"",
      "        node-key=\"${3|每个树节点对应的唯一值|id}\"",
      "        ${3|显示复选框,show-checkbox|}",
      "        ${4|展开树,default-expand-all|}",
      "        >",
      " </el-tree>"
    ],
    "description": ["el-tree树形控件"]
  },
  "el-row(行)": {
    "prefix": ["el-row"],
    "body": [
      "  <el-row ${1:type=\"flex\"} ${2|justify=\"\",align=\"\"|} >",
      "      <el-col :span=\"\"></el-col>",
      "  </el-row>"
    ],
    "description": ["el-row(行)"]
  },
