      "\t\t\t<li><a href=\"javascript:;\">value3</a></li>",
      "\t\t\t<li><a href=\"javascript:;\">value4</a></li>",
      "\t\t</ul>",
      "\t</li>",
      "</ul>",
      "<script>",
      "\tvar nav = document.querySelector('.nav');",
      "\tvar lis = nav.children;",
      "\tfor (var i = 0; i < nav.children.length; I++) {",
      "\t\tlis[i].onmouseover = function () {",
      "\t\t\tthis.children[1].style.display = 'block';",
      "\t\t}",
      "\t\tlis[i].onmouseout = function () {",
      "\t\t\tthis.children[1].style.display = 'none';",
