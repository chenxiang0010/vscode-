  "jQuery电梯导航": {
    "prefix": ["dt", "zj", "html"],
    "body": [
      "// 电梯格式",
      "<div class=\"fixedtool\">",
      "<ul>",
      "\t<li class=\"current\">家用电器</li>",
      "\t<li>手机通讯</li>",
      "\t<li>电脑办公</li>",
      "\t<li>精品家具</li>",
      "</ul>",
      "</div>",
      "// 楼层格式",
      "<div class=\"floor\">",
      "\t<div class=\"w 自定义一楼\"></div>",
      "\t<div class=\"w 自定义二楼\"></div>",
      "\t<div class=\"w 自定义三楼\"></div>",
      "</div>",
      "//页面打开调用判断是否显示电梯导航",
      "getTool();",
      "//判断显示隐藏电梯导航  函数",
      "function getTool(){",
      "\tif ($(document).scrollTop() >= $(\".floor\").offset().top) {",
      "\t\t$(\".fixedtool\").fadeIn();",
      "\t} else {",
      "\t\t$(\".fixedtool\").fadeOut();",
      "\t}",
      "\t//电梯导航切换样式",
      "\t$(\".floor .w\").each(function(i,ele){",
      "\t\tif($(document).scrollTop() >= $(ele).offset().top){",
      "\t\t\t$(\".fixedtool\").find(\"li\").removeClass(\"current\").eq(i).addClass(\"current\")",
      "\t\t}",
      "\t})",
      "}",
      "$(window).scroll(function () {",
      "\tgetTool();",
      "})",
      "//电梯导航操作",
      "$(\".fixedtool\").on(\"click\",\"li\",function(){",
      "\tvar goto = $(\".floor .w\").eq($(this).index()).offset().top;",
      "\t$(\"html\").animate({",
      "\t\tscrollTop:goto",
      "\t})",
      "})"
    ],
    "description": ["jQuery电梯导航"]
  },
